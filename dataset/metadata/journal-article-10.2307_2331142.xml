         <title-group>
            <article-title>Odd-Lot Transactions around the Turn of the Year and the January Effect</article-title>
         </title-group>
         <contrib-group>
            <contrib contrib-type="author">
               <string-name>
                  <given-names>Edward A.</given-names>
